# POS Integration Environment Variables
# Copy this file to .env and fill in your actual credentials

# SuperMenu POS Configuration (Restaurant 1)
SUPERMENU_API_KEY_RESTAURANT_1=your_supermenu_api_key_here
SUPERMENU_API_URL_RESTAURANT_1=https://api.supermenu.com/v1
SUPERMENU_RESTAURANT_ID_1=your_restaurant_id_in_supermenu
SUPERMENU_WEBHOOK_URL_1=https://keyra-api.onrender.com/webhooks/supermenu/restaurant/1

# CheersFood POS Configuration (Restaurant 2)  
CHEERSFOOD_API_KEY_RESTAURANT_2=your_cheersfood_api_key_here
CHEERSFOOD_API_URL_RESTAURANT_2=https://api.cheersfood.com/v1
CHEERSFOOD_RESTAURANT_ID_2=your_restaurant_id_in_cheersfood
CHEERSFOOD_WEBHOOK_URL_2=https://keyra-api.onrender.com/webhooks/cheersfood/restaurant/2

# Future POS Systems - Add as needed:
# SQUARE_API_KEY_RESTAURANT_X=
# TOAST_API_KEY_RESTAURANT_X=
# RESY_API_KEY_RESTAURANT_X=

# POS Integration Settings
POS_TIMEOUT_SECONDS=30
POS_RETRY_ATTEMPTS=3
POS_WEBHOOK_SECRET=your_webhook_secret_for_verification 